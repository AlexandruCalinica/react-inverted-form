{
  "name": "exp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "unlink": "npm unlink react-inverted-form",
    "link": "npm run unlink && cd ../../ npm link && cd ./sandbox/exp && npm link react-inverted-form",
    "copy:dist": "rm -rf ./react-inverted-form && cp -r ../../dist ./react-inverted-form",
    "copy:src": "rm -rf ./react-inverted-form && cp -r ../../src ./react-inverted-form",
    "sync:src": "rm -rf ../../src && cp -r ./react-inverted-form ../../src"
  },
  "dependencies": {
    "class-validator": "^0.13.2",
    "expo": "~46.0.9",
    "expo-status-bar": "~1.4.0",
    "lodash": "^4.17.21",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-web": "~0.18.7",
    "rxjs": "^7.5.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
