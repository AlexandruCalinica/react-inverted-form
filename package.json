{
  "name": "react-inverted-form",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "source": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexandruCalinica/react-inverted-form.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "cz": "cz",
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c ./rollup.config.js && npm run type && npm run copy:assets",
    "copy:assets": "cp ./package.json ./dist/package.json",
    "test:unit": "jest --silent",
    "type": "tsc --project tsconfig.build.json && rollup -c ./scripts/rollup.dts.config.js && rm -rf ./dist/temp",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "class-validator": "^0.13.2",
    "lodash": "^4",
    "react": "^16.8.0 || ^17 || ^18",
    "rxjs": "^7",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@swc/core": "^1.2.215",
    "@swc/jest": "^0.2.21",
    "@types/jest": "28.1.1",
    "@types/lodash": "4.14.182",
    "@types/mocha": "9.1.1",
    "@types/react": "^18.0.17",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "rollup": "^2.78.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "semantic-release": "^19.0.3",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
